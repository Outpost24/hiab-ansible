---
skip_list:
  - meta-no-info
  - risky-file-permissions
  - no-log-password
  - no-changed-when
  - ignore-errors
  - no-jinja-nesting
  - name[casing]
  - name[template]
  - fqcn
  - role-name[path]
  - schema[meta]
  - run-once[task]
  - name[play]
  - var-naming[no-role-prefix]
exclude_paths:
  - .cache
  - .github
  - collections
